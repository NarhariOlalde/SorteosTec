@page
@model Web.Pages.AdminModel
@{
}
<h1>Usuarios</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Correo</th>
            <th>Tarjeta</th>
            <th>Sexo</th>
            <th>Edad</th>
            <th>Localización</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model.Usuarios)
        {
            <tr>
                <td>@usuario.id_usuario</td>
                <td>@usuario.nombre</td>
                <td>@usuario.apellido</td>
                <td>@usuario.correo</td>
                <td>@usuario.datos_bancarios</td>
                <td>@usuario.sexo</td>
                <td>@usuario.edad</td>
                <td>@usuario.localizacion</td>
            </tr>
        }
    </tbody>
</table>